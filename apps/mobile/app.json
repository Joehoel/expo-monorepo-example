{
  "expo": {
    "name": "Boodschappen vergelijker",
    "slug": "boodschappen-vergelijker",
    "version": "1.0.0",
    "scheme": "boodschappen-vergelijker",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "nl.deindruk.vergelijker",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "nl.deindruk.vergelijker",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"]
  }
}
